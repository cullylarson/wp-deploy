{
    "name": "cullylarson/wp-deploy",
    "type": "library",
    "description": "A set of command-line tools for simple deployment of Wordpress installs. Can be used for automated deployment.",
    "keywords": ["wordpress", "command", "sync", "deploy", "deployment"],
    "homepage": "https://github.com/cullylarson/wp-deploy",
    "license": "MIT",
    "authors": [{
        "name": "Cully Larson",
        "email": "cully.larson@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "vlucas/phpdotenv": "~2.1",
        "cullylarson/wp-deploy-database-sync": "~0.0",
        "cullylarson/wp-deploy-folder-sync": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "WpSync\\": "src/WpSync"
        }
    },
    "bin": ["bin/wp-push", "bin/wp-pull"]
}
